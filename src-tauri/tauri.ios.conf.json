{
  "$schema": "https://schema.tauri.app/config/2",
  "bundle": {
    "iOS": {
      "minimumSystemVersion": "14.0"
    }
  },
  "app": {
    "security": {
      "capabilities": [
        {
          "identifier": "default",
          "windows": ["main"],
          "permissions": [
            "core:default",
            "haptics:allow-impact-feedback",
            "haptics:allow-notification-feedback"
          ]
        }
      ]
    }
  }
}
