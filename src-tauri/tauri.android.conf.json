{
  "$schema": "https://schema.tauri.app/config/2",
  "bundle": {
    "android": {
      "minSdkVersion": 24,
      "versionCode": 1
    }
  },
  "app": {
    "security": {
      "capabilities": [
        {
          "identifier": "default",
          "windows": ["main"],
          "permissions": [
            "core:default",
            "haptics:allow-impact-feedback",
            "haptics:allow-vibrate"
          ]
        }
      ]
    }
  }
}
